{
  "expo": {
    "name": "NewMapApp",
    "description": "small map app",
    "slug": "NewMapApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.DistTPatrick.mySmallMapApp",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBhd5w8j4OoN8yjo2uyLOlbd7VPpYVj3EI"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
